<li class="nested_fields">
  <br />
  <%= f.hidden_field :id, :as=>:hidden %>
  <div class="group">
    <%= f.label :name, :class=>"label"%>
    <%= f.text_field :name %>
  </div>
  <div class="group">
    <%= f.label :identifier, :class=>"label"%>
    <%= f.text_field :identifier %>
  </div>
  <div class="group">
    <%= f.label :field_type, :class=>"label" %>
    <%= f.select :field_type, options_for_select(PropertyType::FIELD_TYPES) %>
  </div>
  <div class="group">
    <%= f.label :_destroy, :class=>"label" %>
    <%= f.check_box :_destroy, :class=>"destroy_nested" %>
  </div>
</li>
