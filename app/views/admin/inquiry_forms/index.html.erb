<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_inquiry_forms_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_inquiry_form_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t(".all", :default => "All inquiry forms")  %></h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.inquiry_form.name", :default => t("activerecord.labels.name", :default => "Name")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @inquiry_forms.each do |inquiry_form| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= inquiry_form.id %>
          </td>
          <td>
            <%= link_to inquiry_form.name, admin_inquiry_form_path(inquiry_form) %>
          </td>
          <td>
            <%= inquiry_form.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_inquiry_form_path(inquiry_form) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_inquiry_form_path(inquiry_form) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", admin_inquiry_form_path(inquiry_form), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
